{
    "name": "florddev/laravel-auto-routing",
    "description": "Un package de routage automatique pour Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Florddev\\LaravelAutoRouting\\": "src/"
        }
    },
    "keywords": [
        "laravel",
        "web-development",
        "routing", 
        "laravel-package",
        "backend-development",
        "route-generation",
        "php8-attributes",
        "auto-routing",
        "controller-based-routing"
    ],
    "authors": [
        {
            "name": "Florddev",
            "email": "florddev@protonmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Florddev\\LaravelAutoRouting\\AutoRoutingServiceProvider"
            ]
        }
    }
}
